<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <Description>AdventureWorks store contacts. This report is a subreport used in Sales Order Detail to show all contacts for a store. Borderstyle is None so lines do not display in main report.</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="AdventureWorks">
      <DataSourceReference>AdventureWorks</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4ed0b634-5903-4c1a-8a4c-604bd439124f</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="StoreContacts">
      <Query>
        <DataSourceName>AdventureWorks</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StoreID">
            <Value>=Parameters!StoreID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT s.BusinessEntityID, s.Name as Store, ct.Name as Title, 
   p.FirstName, p.LastName, ph.PhoneNumber
FROM Sales.Store s
	INNER JOIN [Person].[BusinessEntityContact] bec ON bec.[BusinessEntityID] = s.[BusinessEntityID]
        INNER JOIN [Person].[ContactType] ct ON ct.[ContactTypeID] = bec.[ContactTypeID]
        INNER JOIN [Person].[Person] p ON p.[BusinessEntityID] = bec.[PersonID]
	INNER JOIN Person.PersonPhone ph ON p.[BusinessEntityID] = ph.[BusinessEntityID]
	WHERE (s.BusinessEntityID = (@StoreID))
        -- e.g. 642 Advanced Bike Components has a Purchasing Agent and a Purchasing Manager </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="FirstName">
          <DataField>FirstName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BusinessEntityID">
          <DataField>BusinessEntityID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Store">
          <DataField>Store</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastName">
          <DataField>LastName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PhoneNumber">
          <DataField>PhoneNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Stores">
      <Query>
        <DataSourceName>AdventureWorks</DataSourceName>
        <CommandText>SELECT DISTINCT s.BusinessEntityID, s.Name as Store
FROM Sales.Store s
	-- e.g. 642 Advanced Bike Components has a Purchasing Agent and a Purchasing Manager</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="BusinessEntityID">
          <DataField>BusinessEntityID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Store">
          <DataField>Store</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
